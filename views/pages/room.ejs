<script>
	if (typeof participantsNumber!==null){
		var participantsNumber = "<%= participantsNumber %>"
	}
	var roomId = "<%= roomId %>"
	var dbId= "<%= dbId%>"
	var stunUrl="<%=stunUrl%>"
